@model IEnumerable<Stan.SQLData.Stan>

@{
    ViewBag.Title = "Мои станови";
}

<h2>Мои станови</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Kvadratura)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrojNaSobi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NaKojSprat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImaLift)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImaKujna)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImaToalet)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImaTerasa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Namesten)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DatumObjaven)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KontaktIme)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.KontaktBroj)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lokacija)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Opis)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Kvadratura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BrojNaSobi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NaKojSprat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImaLift)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImaKujna)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImaToalet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ImaTerasa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Namesten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatumObjaven)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KontaktIme)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KontaktBroj)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lokacija)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Opis)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
